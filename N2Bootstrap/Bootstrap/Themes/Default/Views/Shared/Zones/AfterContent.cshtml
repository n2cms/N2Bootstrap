@{
    Html.DroppableZone("AfterContent").Render();
    Html.DroppableZone("ContentRecursive").Title("Content Recursive").Render();
    var site = Html.DroppableZone(Content.Traverse.StartPage, "ContentSite");
    if (!Html.IsSiteZoneEditable())
    {
        site.SetState(ControlPanelState.Visible);
    }
    site.Title("Content Site").Render();
}